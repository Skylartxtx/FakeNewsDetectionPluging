{
    "manifest_version": 3,
    "name": "假新闻检测器",
    "version": "1.0",
    "permissions": ["storage"],
    "host_permissions": [
        "http://localhost:5000/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "picture/icon128.png",
            "48": "picture/icon48.png",
            "16": "picture/icon16.png"
        }
    },
    "icons": {
        "128": "picture/icon128.png",
        "48": "picture/icon48.png",
        "16": "picture/icon16.png"
    }

}